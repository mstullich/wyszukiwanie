<Window x:Class="StringSearch.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StringSearch"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="Green">
        <Grid.RowDefinitions>
            <RowDefinition Height="44*"/>
            <RowDefinition Height="173*"/>
        </Grid.RowDefinitions>
        <Label x:Name="BruteLbl" Content="--" HorizontalAlignment="Left" Margin="400,54,0,0" VerticalAlignment="Top" Height="35" Grid.Row="1"/>
        <Button x:Name="SearchBtn" Content="Search" HorizontalAlignment="Left" Margin="538,88,0,0" VerticalAlignment="Top" Click="SearchBtn_Click" Height="30" Width="110" Background="Gray" FontSize="16" FontWeight="Bold" Grid.RowSpan="2"/>
        <TextBox x:Name="SearchBox" HorizontalAlignment="Left" Margin="107,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="370" Height="30" FontSize="16" FontWeight="Normal" Text="Puchatek" Grid.RowSpan="2"/>
        <Label x:Name="AlgoLbl1" Content="Brute Force" HorizontalAlignment="Left" Margin="112,54,0,0" VerticalAlignment="Top" Height="35" Width="96" FontSize="16" FontWeight="Bold" Grid.Row="1"/>



    </Grid>
</Window>
